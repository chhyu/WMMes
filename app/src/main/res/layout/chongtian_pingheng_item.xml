<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TableRow style="@style/tbRowStyleTwo">

        <TextView
            android:id="@+id/tvPosition"
            style="@style/step7_zhusu_text_style"
            android:text="穴号" />


        <EditText
            android:id="@+id/etOne"
            style="@style/step7_zhusu_text_style"
            android:text="1" />

        <EditText
            android:id="@+id/etTwo"
            style="@style/step7_zhusu_text_style"
            android:text="2" />

        <EditText
            android:id="@+id/etThree"
            style="@style/step7_zhusu_text_style"
            android:text="3" />

        <EditText
            android:id="@+id/etFour"
            style="@style/step7_zhusu_text_style"
            android:text="4" />

        <EditText
            android:id="@+id/etFive"
            style="@style/step7_zhusu_text_style"
            android:text="5" />

        <EditText
            android:id="@+id/etSix"
            style="@style/step7_zhusu_text_style"
            android:text="6" />

        <TextView
            android:id="@+id/tvMin"
            style="@style/step7_zhusu_text_style"
            android:text="最小" />

        <TextView
            android:id="@+id/tvMax"
            style="@style/step7_zhusu_text_style"
            android:text="最大" />

        <TextView
            android:id="@+id/tvAverageValue"
            style="@style/step7_zhusu_text_style"
            android:text="平均值" />
    </TableRow>
</TableLayout>